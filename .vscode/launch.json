	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Preferences",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/mrl/dataset/preferences.py",
			"console": "integratedTerminal",
			"args": [
				"traj",
				"--env",
				"miner",
				"--rootdir",
				"data/miner/near-original-reward/7/",
				"--prefs-per-trial",
				"10",
				"--n-trials",
				"1",
				"--n-parallel-envs",
				"10",
				"--outname",
				"debug",
				"--temperature",
				"0",
				"--overwrite"
			]
		}
	]
}